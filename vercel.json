{
  "builds": [
    {
      "src": "AgricultureWholesale/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9",
        "requirementsPath": "./requirements-vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "AgricultureWholesale/wsgi.py"
    }
  ]
} 